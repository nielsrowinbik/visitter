{
  "name": "manage",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env.local -- prisma generate --schema=./manage/lib/db/schema.prisma",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev --schema=./manage/lib/db/schema.prisma",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --schema=./manage/lib/db/schema.prisma",
    "db:push": "dotenv -e .env.local -- prisma db push --schema=./manage/lib/db/schema.prisma",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:studio": "dotenv -e .env.local -- prisma studio --schema=./manage/lib/db/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./manage/lib/db/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@hookform/error-message": "^2.0.0",
    "@mantine/core": "^5.1.4",
    "@mantine/dates": "^5.1.4",
    "@mantine/hooks": "^5.1.4",
    "@mantine/next": "^5.1.4",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.2.1",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.1",
    "dayjs": "^1.11.4",
    "firebase": "^9.9.2",
    "firebase-admin": "^11.0.1",
    "lodash": "^4.17.21",
    "next": "12.2.3",
    "next-auth": "^4.10.3",
    "next-connect": "^0.13.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.34.0",
    "react-query": "^3.39.2",
    "superagent": "^8.0.0",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.184",
    "@types/node": "^17.0.12",
    "@types/react": "17.0.47",
    "@types/uuid4": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "dotenv-cli": "^6.0.0",
    "eslint": "7.32.0",
    "eslint-config-custom": "workspace:*",
    "next-transpile-modules": "9.0.0",
    "postcss": "^8.4.16",
    "prisma": "^4.2.1",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3"
  }
}
