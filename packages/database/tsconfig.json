{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "commonjs",
    "target": "ES2021",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "tsup.config.ts"],
  "exclude": ["node_modules"]
}
